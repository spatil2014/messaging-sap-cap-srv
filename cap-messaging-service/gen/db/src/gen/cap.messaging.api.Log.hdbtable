COLUMN TABLE cap_messaging_api_Log (
  ID NVARCHAR(36) NOT NULL,
  createdAt TIMESTAMP,
  createdBy NVARCHAR(255),
  modifiedAt TIMESTAMP,
  modifiedBy NVARCHAR(255),
  vendor NVARCHAR(60) NOT NULL,
  msgType NVARCHAR(4) NOT NULL,
  documentType NVARCHAR(20) NOT NULL,
  documentRef NVARCHAR(100) NOT NULL,
  status NVARCHAR(30),
  messageContent NCLOB,
  PRIMARY KEY(ID, vendor, msgType, documentType, documentRef)
)