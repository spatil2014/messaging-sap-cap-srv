VIEW MessagingService_recipient AS SELECT
  Recipient_0.ID,
  Recipient_0.message_ID,
  Recipient_0.channel,
  Recipient_0.contact,
  Recipient_0.email,
  Recipient_0.status
FROM cap_messaging_api_Recipient AS Recipient_0
WITH ASSOCIATIONS (
  MANY TO ONE JOIN MessagingService_messages AS message ON (message.ID = message_ID)
)