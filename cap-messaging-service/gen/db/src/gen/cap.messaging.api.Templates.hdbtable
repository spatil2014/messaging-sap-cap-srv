COLUMN TABLE cap_messaging_api_Templates (
  createdAt TIMESTAMP,
  createdBy NVARCHAR(255),
  modifiedAt TIMESTAMP,
  modifiedBy NVARCHAR(255),
  vendor NVARCHAR(60) NOT NULL,
  documentType NVARCHAR(20) NOT NULL,
  msgType NVARCHAR(4) NOT NULL,
  templateID NVARCHAR(50),
  content NVARCHAR(200),
  templateOrder NVARCHAR(6),
  PRIMARY KEY(vendor, documentType, msgType)
)