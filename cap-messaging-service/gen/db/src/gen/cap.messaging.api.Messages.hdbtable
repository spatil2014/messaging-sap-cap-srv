COLUMN TABLE cap_messaging_api_Messages (
  ID NVARCHAR(36) NOT NULL,
  content NVARCHAR(5000),
  subject NVARCHAR(5000),
  templateID BIGINT,
  attachment VARBINARY(5000),
  document_no NVARCHAR(5000),
  emails NCLOB,
  channel NVARCHAR(5000),
  isSignRequired BOOLEAN,
  placeholders NCLOB,
  PRIMARY KEY(ID)
)