COLUMN TABLE cap_messaging_api_Configuration (
  createdAt TIMESTAMP,
  createdBy NVARCHAR(255),
  modifiedAt TIMESTAMP,
  modifiedBy NVARCHAR(255),
  vendor NVARCHAR(60) NOT NULL,
  msgType NVARCHAR(4) NOT NULL,
  apihost NVARCHAR(200),
  port SMALLINT,
  username NVARCHAR(50),
  password NVARCHAR(30),
  sender NVARCHAR(50),
  documentType NVARCHAR(20),
  active BOOLEAN DEFAULT TRUE,
  PRIMARY KEY(vendor, msgType)
)